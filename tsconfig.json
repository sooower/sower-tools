{
    "compilerOptions": {
        "module": "Node16",
        "target": "ES2022",
        "lib": ["ES2022"],
        "sourceMap": true,
        "rootDir": ".",
        "strict": true,
        "paths": {
            "@/*": ["./src/*"],
            "@lib/*": ["./lib/*"]
        },
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true
    }
}
